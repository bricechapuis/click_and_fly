<div class="wrapper">
  <div class="container main-page">
    <div class="main-title"><h1>Click & Fly</h1></div>
      <form class="form-inline">
        <div class="form-group mx-sm-3 mb-2">
          <label for="inputRegion" class="sr-only">Region</label>
          <input type="password" class="form-control" id="inputRegion" placeholder="Region">
        </div>
        <div class="form-group mx-sm-3 mb-2">
          <label for="inputStartDate" class="sr-only">Start</label>
          <input type="password" class="form-control" id="inputStartDate" placeholder="Start">
        </div>
        <div class="form-group mx-sm-3 mb-2">
          <label for="inputEndDate" class="sr-only">End</label>
          <input type="password" class="form-control" id="inputEndDate" placeholder="End">
        </div>
        <div class="form-group mx-sm-3 mb-2">
          <label for="inputCapacity" class="sr-only">Capacity</label>
          <input type="password" class="form-control" id="inputCapacity" placeholder="End">
        </div>
        <button type="submit" class="btn btn-primary mb-2">Go!</button>
      </form>
    </div>
  </div>
</div>
